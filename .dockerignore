# Ignore dependency folders
/vendor
/node_modules

# Ignore environment files
.env
.env.*  # all env variants

# Ignore Git files
.git
.gitignore
.gitattributes

# Ignore OS/editor files
.DS_Store
Thumbs.db
*.log

# Ignore Laravel caches (will be rebuilt)
bootstrap/cache/*.php

# Ignore storage except public uploads
storage/*.key
storage/logs
storage/framework/cache
storage/framework/sessions
storage/framework/testing
